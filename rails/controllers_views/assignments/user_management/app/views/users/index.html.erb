<div class="container">
	<h1> Listing Users </h1>

	<table class="table"> 
		<thead>
			<tr>
				<th> First Name </th>
				<th> Last Name </th>
				<th> Email </th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
			<tr> 
				<td> <%= user.first_name %> </td>
				<td> <%= user.last_name %> </td>
				<td> <%= user.email_address %>  </td>
			</tr>
			<% end %>
		</tbody>

	</table>
</div>